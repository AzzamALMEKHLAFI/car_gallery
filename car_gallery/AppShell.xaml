<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="car_gallery.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:car_gallery"
    Shell.FlyoutBehavior="Flyout">
    
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Müşteri"
        ContentTemplate="{DataTemplate local:carsList}"
        Route="carsList" />

    <ShellContent
        Title="saticilar"
        ContentTemplate="{DataTemplate local:saticilarList}"
        Route="saticilarList" />
    
    <ShellContent
        Title="Marka"
        ContentTemplate="{DataTemplate local:MarkaList}"
        Route="MarkaList" />
    
    <ShellContent
        Title="Satış "
        ContentTemplate="{DataTemplate local:satisDetya}"
        Route="satisDetya" />
        
    <ShellContent
        Title="Araba"
        ContentTemplate="{DataTemplate local:arabaDetay}"
        Route="arabaDetay" />



</Shell>
